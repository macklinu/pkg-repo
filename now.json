{
  "version": 2,
  "name": "repo",
  "alias": "repo",
  "public": true,
  "builds": [{ "src": "functions/*.js", "use": "@now/node" }],
  "routes": [
    { "src": "^/$", "methods": ["GET"], "dest": "/functions/index.js" },
    { "src": "^/(.*)$", "methods": ["GET"], "dest": "/functions/pkg.js" }
  ],
  "github": {
    "enabled": true
  }
}
